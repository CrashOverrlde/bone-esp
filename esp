	for _,v in pairs(game.Players:GetPlayers()) do
		if workspace.CurrentCamera.CameraType == Enum.CameraType.Custom then
			if v.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				delay(1.5, function()
					repeat wait() until v.Character

					for i, v in pairs(v.Character:GetChildren()) do
						if v:IsA("Shirt") or v:IsA("Pants") then
							v:Destroy()
						end
					end

					for i, v in pairs(v.Character:GetChildren()) do
						if v:IsA("Accessory") then
							v:Destroy()
						end
					end

					for i, v in pairs(v.Character:GetChildren()) do
						if v:IsA("MeshPart") then
							v.Transparency = 0.5
						end
					end
				end)
			end

			if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Character:WaitForChild("Humanoid").RigType == Enum.HumanoidRigType.R6 and game.PlaceId == 6452089755 and v.Character:FindFirstChildOfClass("Tool") and not v.Character:FindFirstChild("Box") then
				print("updating framework...")

				local Name = Instance.new("BillboardGui")
				local TextLabel = Instance.new("TextLabel")
				local Mag = Instance.new("BillboardGui")
				local TextLabel_2 = Instance.new("TextLabel")

				Name.Name = "Name"
				Name.Parent = v.Character.Head
				Name.Active = true
				Name.Adornee = v.Character.Head
				Name.AlwaysOnTop = true
				Name.ExtentsOffset = Vector3.new(0, 2, 0)
				Name.LightInfluence = 1
				Name.Size = UDim2.new(0, 600, 0, 20)
				Name.StudsOffset = Vector3.new(0, 0.40000000596046, 1)

				TextLabel.Parent = Name
				TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
				TextLabel.BackgroundTransparency = 1
				TextLabel.Size = UDim2.new(1, 0, 1, 0)
				TextLabel.ZIndex = 2
				TextLabel.Font = Enum.Font.PermanentMarker
				TextLabel.TextColor3 = Color3.new(1, 0.168627, 0.960784)
				TextLabel.TextScaled = true
				TextLabel.TextSize = 14
				TextLabel.TextWrapped = true

				Mag.Name = "Mag"
				Mag.Parent = v.Character.Head
				Mag.Active = true
				Mag.Adornee = v.Character.Head
				Mag.AlwaysOnTop = true
				Mag.ExtentsOffset = Vector3.new(0, 2, 0)
				Mag.LightInfluence = 1
				Mag.Size = UDim2.new(0, 400, 0, 20)
				Mag.StudsOffset = Vector3.new(0, -1, 0)

				TextLabel_2.Parent = Mag
				TextLabel_2.BackgroundColor3 = Color3.new(1, 1, 1)
				TextLabel_2.BackgroundTransparency = 1
				TextLabel_2.Size = UDim2.new(1, 0, 1, 0)
