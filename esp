local function ZigZag(X) return math.acos(math.cos(X * math.pi)) / math.pi end

local count = 0

game:GetService("RunService").RenderStepped:Connect(function(DT)
	for i, v in pairs(FJEWIFi93284juIJVEFHWUOFYE:GetChildren()) do
		if v:IsA("TextLabel") then
			v.TextColor3 = Color3.fromHSV(ZigZag(count) ,1,1)
			count = count + 0.01
		end
	end

	delay(.05, function()
		local Tween1 = game.TweenService:Create(L1, TweenInfo.new(2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 0, 0)})
		Tween1:Play()
		Tween1.Completed:Wait()
		local Tween2 = game.TweenService:Create(L1, TweenInfo.new(2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 0, 0)})
		Tween2:Play()
		Tween2.Completed:Wait()
	end)
	delay(.1, function()
		local Tween1 = game.TweenService:Create(L2, TweenInfo.new(2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 0, 0)})
		Tween1:Play()
		Tween1.Completed:Wait()
		local Tween2 = game.TweenService:Create(L2, TweenInfo.new(2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 0, 0)})
