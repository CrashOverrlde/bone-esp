					else
						if v.Character:WaitForChild("Humanoid").RigType == Enum.HumanoidRigType.R15 and not v.Character:FindFirstChild("Box") then
							repeat wait() until v.Character

							for i, v in pairs(v.Character:GetChildren()) do
								if v:IsA("Shirt") or v:IsA("Pants") then
									v:Destroy()
								end
							end

							for i, v in pairs(v.Character:GetChildren()) do
								if v:IsA("Accessory") then
									v:Destroy()
								end
							end

							for i, v in pairs(v.Character:GetChildren()) do
								if v:IsA("MeshPart") then
									v.Transparency = 0.5
								end
							end

